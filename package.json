{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "generate-sitemap": "node generateSitemap.js",
    "static-export": "node static-export.js",
