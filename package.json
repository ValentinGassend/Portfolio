    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^6.23.0",
    "swiper": "^11.1.1",
    "three": "^0.165.0",
    "vite-plugin-imagemin": "^0.6.1",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
